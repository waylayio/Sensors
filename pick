{
  "name": "pick",
  "version": "0.0.4",
  "type": "sensor",
  "script": "const object = waylayUtil.getProperty(options, 'object')\nconst properties = waylayUtil.getProperty(options, 'properties')\nconst paths = properties.toString().split(',')\n\nconst data = _.pick(object, paths)\n\nconst allPropsCollected = !_.some(paths, path => object[path] === undefined)\n\nsend(null, {\n  observedState: allPropsCollected ? 'Collected' : 'Not Collected',\n  rawData: { data }\n})\n",
  "metadata": {
    "author": "Ward",
    "category": "General",
    "description": "Picks properties from the provided object and returns them in rawData.data\n\nState is Collected only when all properties are collected.\n\n<ul>\n  <li>Properties: comma separated list of required properties</li>\n  <li>Object: the object in which to look for these properties</li>\n</ul>",
    "iconURL": "https://s3.eu-central-1.amazonaws.com/static.waylay.io/plugs/icons/pick.png",
    "supportedStates": [
      "Collected",
      "Not Collected"
    ],
    "requiredProperties": [
      "properties",
      "object"
    ],
    "requiredRawData": [],
    "rawData": [
      {
        "parameter": "data",
        "dataType": "object"
      }
